- if leave.current_status == "Approved"
  %tr
    %td= leave.created_at.strftime("%d-%m-%Y")
    %td= leave.no_of_days
    %td= leave.start_date.strftime("%d-%m-%Y")
    %td= leave.end_date.strftime("%d-%m-%Y")
    %td= leave.updated_at.strftime("%d-%m-%Y")
    %td
      - @users.each do |user|
        = leave.user.manager.email






